<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.java.easybuy.entity.OrderEntity" table="easybuy_order" schema="easybuy">
        <id name="id" column="id"/>
        <property name="userId" column="userId"/>
        <property name="loginName" column="loginName"/>
        <property name="userAddress" column="userAddress"/>
        <property name="createTime" column="createTime"/>
        <property name="cost" column="cost"/>
        <property name="serialNumber" column="serialNumber"/>
    </class>
</hibernate-mapping>